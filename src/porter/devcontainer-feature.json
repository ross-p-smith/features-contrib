{
    "id": "porter",
    "version": "0.0.15",
    "name": "porter",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/porter",
    "description": "Porter packages your application, client tools, configuration, and deployment logic into an installer that you can distribute and run with a single command.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        },
        "mixins": {
            "default": "",
            "description": "Select the mixins to install.",
            "type": "string"
        },
        "plugins": {
            "default": "",
            "description": "Select the plugins to install.",
            "proposals": "azure",
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/docker-outside-of-docker",
        "ghcr.io/devcontainers/features/docker-in-docker"
    ]
}